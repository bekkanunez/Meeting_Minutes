iv>\n            ) : (\n              <MeetingList\n                meetings={meetings}\n                title=\"Upcoming meetings...\"\n              />\n            )}\n          </div>\n      </div>\n    </main>\n  );\n};\n\nexport default Home;\n","/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/pages/Login.js",[],"/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/components/Header/index.js",[],"/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/components/Footer/index.js",[],"/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/utils/auth.js",[],"/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/utils/queries.js",[],"/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/utils/mutations.js",[],"/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/components/MeetingForm/index.js",["71"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_MEETING } from '../../utils/mutations';\nimport { QUERY_MEETINGS, QUERY_ME } from '../../utils/queries';\n\nimport Auth from '../../utils/auth';\n\nconst MeetingForm = () => {\n  const [meetingText, setMeetingText] = useState('');\n\n  const [characterCount, setCharacterCount] = useState(0);\n\n  const [addMeeting, { error }] = useMutation(ADD_MEETING, {\n    update(cache, { data: { addMeeting } }) {\n      try {\n        const { meetings } = cache.readQuery({ query: QUERY_MEETINGS });\n\n        cache.writeQuery({\n          query: QUERY_MEETINGS,\n          data: { meetings: [addMeeting, ...meetings] },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n\n      // update me object's cache\n      const { me } = cache.readQuery({ query: QUERY_ME });\n      cache.writeQuery({\n        query: QUERY_ME,\n        data: { me: { ...me, meetings: [...me.meetings, addMeeting] } },\n      });\n    },\n  });\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addMeeting({\n        variables: {\n          meetingText,\n          meetingHost: Auth.getProfile().data.username,\n        },\n      });\n\n      setMeetingText('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === 'meetingText' && value.length <= 280) {\n      setMeetingText(value);\n      setCharacterCount(value.length);\n    }\n  };\n\n  return (\n    <div>\n      <h3>Would you like to create a new meeting?</h3>\n\n      {Auth.loggedIn() ? (\n        <>\n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center py-2\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <input\n                name=\"meetingHost\"\n                type=\"text\"\n                placeholder=\"Who will host the meeting?\"\n                className=\"form-input w-100\"\n                style={{ lineHeight: '.8', resize: 'vertical' }}\n                onChange={handleChange}\n              ></input>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <input\n                name=\"department\"\n                type=\"text\"\n                placeholder=\"What department?\"\n                className=\"form-input w-100\"\n                style={{ lineHeight: '.8', resize: 'vertical' }}\n                onChange={handleChange}\n              ></input>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <input\n                name=\"title\"\n                type=\"text\"\n                placeholder=\"What is the subject meeting title?\"\n                className=\"form-input w-100\"\n                style={{ lineHeight: '.8', resize: 'vertical' }}\n                onChange={handleChange}\n              ></input>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n            <input\n                name=\"date\"\n                type=\"datetime-local\"\n                placeholder=\"What is the meeting date?\"\n                className=\"form-input w-100\"\n                style={{ lineHeight: '.8', resize: 'vertical' }}\n                onChange={handleChange}\n              ></input>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"meetingText\"\n                placeholder=\"Brief description...\"\n                value={meetingText}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n              <p\n              className={`mx-5 mt-2 ${characterCount === 280 || error ? 'text-danger' : ''\n                }`}\n            >\n              Character Count: {characterCount}/280\n            </p>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Meeting\n              </button>\n            </div>\n            {error && (\n              <div className=\"col-12 my-3 bg-danger text-white p-3\">\n                {error.message}\n              </div>\n            )}\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to post and view meetings. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default MeetingForm;\n","/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/components/MeetingList/index.js",[],"/Users/drewparker/Desktop/ut-bootcamp/Project-3/client/src/pages/SingleMeeting.js",[],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"72","replacedBy":"76"},{"ruleId":"74","replacedBy":"